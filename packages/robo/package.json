{
	"name": "@roboplay/robo.js",
	"version": "0.9.0",
	"private": false,
	"description": "Turbocharge Discord.js with effortless power!",
	"main": "dist/index.js",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"build": "tsup && tsup --config tsup-cli.config.ts",
		"dev": "chokidar \"src/**/*.ts\" -d 1000 -c \"pnpm build\" --initial"
	},
	"bin": {
		"robo": "dist/cli/index.js"
	},
	"type": "module",
	"license": "MIT",
	"files": [
		"dist/",
		"src/",
		"LICENSE",
		"README.md",
		"types.js",
		"types.d.ts"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Wave-Play/robo.git",
		"directory": "packages/discord"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {},
	"devDependencies": {
		"@swc/core": "1.3.41",
		"@types/node": "18.15.5",
		"chokidar-cli": "^3.0.0",
		"discord.js": "14.11.0",
		"keyv": "^4.5.2",
		"tsup": "6.7.0",
		"typescript": "5.0.2"
	},
	"peerDependencies": {
		"@swc/core": "^1.3.41",
		"discord.js": "^14.0.0",
		"keyv": "^4.5.2",
		"typescript": "^5.0.0"
	},
	"peerDependenciesMeta": {
		"@swc/core": {
			"optional": true
		},
		"discord.js": {
			"optional": false
		},
		"keyv": {
			"optional": true
		},
		"typescript": {
			"optional": true
		}
	}
}
